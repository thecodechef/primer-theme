{
    "name": "primer-theme",
    "displayName": "Primer Theme",
    "icon": "images/logo_128x128.png",
    "description": "A port of the Sublime Text Plugin \"Theme - Primer\".",
    "version": "0.0.5",
    "publisher": "cyberchefjay",
    "license": "MIT",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Themes"
    ],
    "repository": {
        "type": "git",
        "url": "http://www.github.com/thecodechef/primer-theme.git"
    },
    "contributes": {
        "themes": [{
                "label": "Primer (dark)",
                "uiTheme": "vs-dark",
                "path": "./themes/Primer (dark).tmTheme"
            },
            {
                "label": "Primer (light)",
                "uiTheme": "vs",
                "path": "./themes/Primer (light).tmTheme"
            },
            {
                "label": "Primer (light2)",
                "uiTheme": "vs",
                "path": "./themes/Primer (light2).tmTheme"
            }
        ]
    },
    "dependencies": {
        "vsce": "^1.20.0"
    },
    "devDependencies": {
        "gulp": "^3.9.1",
        "gulp-data": "^1.2.1",
        "gulp-flatmap": "^1.0.0",
        "gulp-pug": "^4.0.1",
        "gulp-rename": "^1.2.2"
    }
}